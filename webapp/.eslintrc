{
  "extends": "angular",
  "env": {
	"es6": true,
	//	"jasmine": true,
	"node": true
	//	"mocha": true,
	//	"browser": true,
	//	"builtin": true
  },
  "ecmaFeatures": {
	"modules": true
  },
  "globals": {},
  "rules": {
	"angular/module-getter": 0,
	"angular/controller-as": 2,
	"angular/controller-as-vm": 0,
	"angular/no-service-method": 0,
	"angular/document-service": 1,
	"no-use-before-define": 0,
	"block-scoped-var": 2,
	"camelcase": 1,
	"comma-style": [
	  1,
	  "last"
	],
	"curly": [
	  2,
	  "all"
	],
	"dot-notation": [
	  2,
	  {
		"allowKeywords": true
	  }
	],
	"eqeqeq": [
	  1,
	  "allow-null"
	],
	"guard-for-in": 2,
	"new-cap": 0,
	"no-bitwise": 2,
	"no-caller": 2,
	"no-cond-assign": [
	  2,
	  "except-parens"
	],
	"no-debugger": 2,
	"no-empty": 2,
	"no-eval": 2,
	"no-extend-native": 2,
	"no-extra-parens": 1,
	"no-irregular-whitespace": 2,
	"no-iterator": 2,
	"no-loop-func": 2,
	"no-multi-str": 2,
	"no-new": 2,
	"no-plusplus": 0,
	"no-proto": 2,
	"no-script-url": 2,
	"no-sequences": 2,
	"no-shadow": 1,
	"no-undef": 1,
	"no-unused-vars": 1,
	"no-with": 2,
	"quotes": [
	  1,
	  "double"
	],
	"semi": [
	  0,
	  "never"
	],
	"strict": [
	  2,
	  "global"
	],
	"valid-typeof": 2,
	"wrap-iife": [
	  2,
	  "inside"
	]
  }
}